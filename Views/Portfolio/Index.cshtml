    <!-- //home page video popup section -->
    <!-- freelance hire me -->
    <section class="w3l-grid-quote text-center py-5">
        <div class="container py-3">
            <h6 class="title-small">Temasta Olalım</h6>
            <h3 class="title-big mb-md-5 mb-4">Bir Proje Başlatalım! Beni işe al.</h3>
            <a href="/Contact/Index" class="btn btn-style btn-primary mr-2">Beni işe al </a>

