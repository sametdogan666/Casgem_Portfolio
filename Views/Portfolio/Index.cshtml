    <!-- home page second section -->
    @Html.Action("PartialMyResume");
    <!-- //home page second section -->
    <!-- home page services section -->
    @Html.Action("PartialService");
    <!-- //home page services section -->
    <!-- stats -->
    @Html.Action("PartialStatistics");
    <!-- //stats -->
